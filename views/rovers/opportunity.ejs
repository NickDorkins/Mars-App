<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../templates/head') %>
</head>
<body>
  <header> 
    <%- include('../templates/header') %>
  </header>
  <div class="randomize">
    <button><a href="/rovers/opportunity">Randomize New Sol</a></button>
  </div>
  <% results.forEach(results => { %>
    <div class="roverPics">
     <img src="<%= results.image %>">
     <h2>Sol: <%= results.sol %></h2>
     <h2>Earth Date: <%= results.date %></h2>
     <h2>Camera Name: <%= results.camera %></h2>
    </div>
  <% }) %>

</body>
</html>